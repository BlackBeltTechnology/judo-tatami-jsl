        t.name = s.name + "::Refresh";
        t.actionDefinition = s.getUpdateViewModifier().view.equivalent("ViewPageContainerRefreshActionDefinition");

        log.debug("ViewTableViewPageDefinitionRefreshAction: " + t.name);
}


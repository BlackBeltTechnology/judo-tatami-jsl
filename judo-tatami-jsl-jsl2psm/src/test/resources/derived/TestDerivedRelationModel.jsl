model DerivedRelationModel

type numeric Integer precision 9  scale 0
type boolean Boolean

type string String max-length 128

entity Lead {
	field Integer value
	field Customer[] customers
	derived Customer[] keyCustomers = self.customers!filter(c | c.isKey)
	derived Customer keyCustomer = self.customers!filter(c | c.isKey)!any()
}

entity Customer {
	field String name
	field Boolean iskey
}

entity LeadExtended extends Lead {
}

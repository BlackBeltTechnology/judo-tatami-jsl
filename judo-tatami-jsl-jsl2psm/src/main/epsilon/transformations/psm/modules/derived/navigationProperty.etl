
rule CreateNavigationProperty
    transform s : JSL!EntityCalculatedRelationDeclaration
    to t : JUDOPSM!NavigationProperty {
      guard: not s.isQuery() and s.getReferenceType().isKindOf(JSL!EntityDeclaration)
      t.name = s.name;
      t.setId("(jsl/" + s.getId() + ")/CreateNavigationProperty");
      t.getterExpression = s.equivalent("CreateGetterExpressionForReferenceType");
      t.cardinality = s.equivalent("CreateCardinalityForDerivedDeclaration");
      t.target = s.getReferenceType().getPSMEquivalent();

      s.eContainer.getPSMEquivalent().navigationProperties.add(t);
      log.debug("Created Navigation Property: " + t.name);
}

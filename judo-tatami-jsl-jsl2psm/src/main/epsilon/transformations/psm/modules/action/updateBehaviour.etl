@lazy
@greedy
rule CreateCardinalityForUpdateTransferTypeInput
    transform s : JSL!TransferUpdateDeclaration
    to t : JUDOPSM!Cardinality {
      t.setId("(jsl/" + s.getId() + ")/CreateCardinalityForUpdateTransferTypeInput");

      t.lower = 1;
      t.upper = 1;
}

